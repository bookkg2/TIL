<script>
$(document).ready(function(){
	var center = new google.maps.LatLng(34.865290, 128.434132);
	var map = new google.maps.Map(
		document.querySelector('#map'),
		{
		mapTypeId:google.maps.MapTypeId.ROADMAP,
		zoom:17,
		center:center
		}
		);
});
</script>
