<script>
function makeChart(data){
	Highcharts.chart('container', {
    chart: {
        type: 'line'
    },
    title: {
        text: 'Seoul Monthly Average Temperature'
    },
    subtitle: {
        text: ''
    },
    xAxis: {
        categories: ['Jan', 'Feb', 'Mar', 'Apr', 'May', 'Jun', 'Jul', 'Aug', 'Sep', 'Oct', 'Nov', 'Dec']
    },
    yAxis: {
        title: {
            text: 'Temperature (Â°C)'
        }
    },
    plotOptions: {
        line: {
            dataLabels: {
                enabled: true
            },
            enableMouseTracking: false
        }
    },
    series:data
	});
}; // end makeChart

function Temp(n,d1){
	this.name = n;
	this.data = d1;
};

