<script>
$(document).ready(function(){
	if(typeof(EventSource) !== 'undefined'){
		var app = new EventSource('test.jsp');
		app.onmessage = function(ev){
			$('h3').text(ev.data);
		};
	}else{
		alert('Server Event Not Available');
	};
});
</script>
